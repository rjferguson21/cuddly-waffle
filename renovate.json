{
  "extends": [
    "github>rjferguson21/uds-renovate//renovate.json5",
    "group:all",
    "replacements:all",
    "workarounds:all"
  ],
  "hostRules": [
    {
      "matchHost": "registry1.dso.mil",
      "hostType": "docker",
      "description": "Encrypted creds for registry1, scoped to this Github org using: https://github.com/renovatebot/renovate/blob/main/docs/usage/configuration-options.md#encrypted",
      "encrypted": {
        "username": "",
        "password": ""
      }
    }
  ],
  "packageRules": [
    {
      "packagePatterns": ["*"],
      "groupName": "Other Dependencies",
      "labels": ["all-other-deps"]
    },
    {
      "matchDatasources": ["docker"],
      "groupName": "Gitlab Runner Specific Dependencies",
      "registryUrls": ["https://registry1.dso.mil"],
      "labels": ["gitlab-runner-deps"]
    },
    {
      "matchDatasources": ["helm"],
      "groupName": "Gitlab Runner Specific Dependencies",
      "labels": ["gitlab-runner-deps"]
    }
  ]
}
